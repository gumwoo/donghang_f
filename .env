# Docker Compose 환경변수
COMPOSE_PROJECT_NAME=donghang

# 데이터베이스 설정
POSTGRES_DB=donghang_db
POSTGRES_USER=donghang_user
POSTGRES_PASSWORD=donghang_password

# JWT 설정
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRE=24h

# API URLs
REACT_APP_API_URL=http://localhost:5000
REACT_APP_AI_API_URL=http://localhost:8000

# PWA 설정
REACT_APP_PWA_NAME=금복이
REACT_APP_PWA_SHORT_NAME=금복이

# AI 모델 설정
MODEL_NAME=microsoft/DialoGPT-medium
MAX_LENGTH=512
TEMPERATURE=0.7
